{
	"manifest_version": 2,
	"name": "TV Show Manager",
	"version": "1",
	"app": {
		"background": {
			"scripts": ["background.js","js/database.js"]
		}
	},
	"offline_enabled": true,
	"permissions": [
		"storage","notifications","http://*.appspot.com/","http://trakt.us/","http://*.trakt.tv/"
	],
	"icons": { "16": "tv16.png", "128": "tv128.png" }
}